<p id="notice"><%= notice %></p>

<h1>Framings</h1>

<table>
  <thead>
    <tr>
      <th>Service time</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @framings.each do |framing| %>
      <tr>
        <td><%= framing.service_time %></td>
        <td><%= framing.position %></td>
        <td><%= link_to 'Show', framing %></td>
        <td><%= link_to 'Edit', edit_framing_path(framing) %></td>
        <td><%= link_to 'Destroy', framing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Framing', new_framing_path %>
